{
  "name": "qrgen",
  "version": "2.1.13",
  "description": "QrGen.cc is a free service that lets you create QR-Codes and shortened URLs from any link quickly and easily",
  "main": "build/index.js",
  "scripts": {
    "client:serve": "cd client && npx vue-cli-service serve",
    "client:build": "cd client && npx vue-cli-service build",
    "client:lint": "cd client && npx vue-cli-service lint",
    "server:dev": "./node_modules/.bin/tsc-watch --onSuccess \"nodemon ./build/index.js\"",
    "server:build": "tsc -p tsconfig.json",
    "server:lint": "eslint ./src/ --ext .ts",
    "dependencies": "npm install && cd client && npm install",
    "build": "npm run server:build && npm run client:build",
    "start": "node ./build/index.js",
    "lint": "npm run server:lint && npm run client:lint",
    "test": "nyc --reporter=html --reporter=text ava --verbose",
    "coverage": "nyc --reporter=lcov ava",
    "semantic-release": "semantic-release",
    "pm2": "sudo pm2 start ./build/index.js --name qrgen.cc"
  },
  "author": "Maximilian Schiller <schiller@mxis.ch>",
  "repository": {
    "type": "git",
    "url": "https://github.com/BetaHuhn/qrgen.git"
  },
  "bugs": {
    "url": "https://github.com/BetaHuhn/qrgen/issues"
  },
  "homepage": "https://qrgen.cc",
  "license": "MIT",
  "dependencies": {
    "@dotvirus/yxc": "0.0.6",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "debug": "^4.3.1",
    "dotenv": "^8.2.0",
    "ejs": "^3.1.6",
    "express": "^4.17.1",
    "express-rate-limit": "^5.2.6",
    "metadata-scraper": "^0.2.16",
    "mongoose": "^5.12.3",
    "nanoid": "^3.1.22",
    "request": "^2.88.2",
    "request-ip": "^2.1.3",
    "running-at": "^0.3.14",
    "signale": "^1.4.0",
    "url-exists-deep": "^2.0.6"
  },
  "devDependencies": {
    "@babel/core": "^7.13.14",
    "@babel/node": "^7.13.13",
    "@betahuhn/config": "^1.0.2",
    "@types/compression": "^1.7.0",
    "@types/cors": "^2.8.10",
    "@types/debug": "^4.1.5",
    "@types/ejs": "^3.0.6",
    "@types/express": "^4.17.11",
    "@types/express-rate-limit": "^5.1.1",
    "@types/nanoid": "^2.1.0",
    "@types/node": "^14.14.37",
    "@types/signale": "^1.4.1",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^4.20.0",
    "@typescript-eslint/parser": "^4.20.0",
    "ava": "^3.15.0",
    "coveralls": "^3.1.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.23.0",
    "mongodb-memory-server": "^6.9.6",
    "nodemon": "^2.0.7",
    "nyc": "^15.1.0",
    "supertest": "^6.1.3",
    "ts-node": "^9.1.1",
    "tsc-watch": "^4.2.8",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.2.3"
  },
  "ava": {
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register",
      "tsconfig-paths/register"
    ]
  }
}
